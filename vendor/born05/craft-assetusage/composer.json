{
    "name": "born05/craft-assetusage",
    "description": "Adds a column to see which assets are used or unused.",
    "type": "craft-plugin",
    "version": "2.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "asset usage"
    ],
    "support": {
        "docs": "https://github.com/born05/craft-assetusage/blob/master/README.md",
        "issues": "https://github.com/born05/craft-assetusage/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Born05",
            "homepage": "https://www.born05.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "born05\\assetusage\\": "src/"
        }
    },
    "extra": {
        "name": "Asset Usage",
        "handle": "assetusage",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/born05/craft-assetusage/master/CHANGELOG.md"
    }
}
